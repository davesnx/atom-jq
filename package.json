{
  "name": "atom-jq",
  "main": "./lib/index.js",
  "description": "A playground for jq inside atom",
  "version": "0.7.0",
  "license": "MIT",
  "activationCommands": {},
  "repository": "https://github.com/sanack/atom-jq",
  "scripts": {
    "lint": "snazzy lib/ test/ spec/",
    "pretest": "npm run lint",
    "test": "babel-tape-runner ./test/*.test.js | tap-spec",
    "test:w": "nodemon --quiet --exec npm run test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "engines": {
    "atom": "*"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "node-jq": "1.9.1",
    "path": "^0.12.7",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-tape-runner": "^2.0.1",
    "condition-circle": "^1.5.0",
    "enzyme": "^2.3.0",
    "ghooks": "^1.3.2",
    "react-addons-test-utils": "^15.2.0",
    "require-dir": "^0.3.0",
    "semantic-release": "^6.3.2",
    "snazzy": "^4.0.0",
    "standard": "^13.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "validate-commit-msg": "^2.8.2"
  },
  "providedServices": {},
  "configSchema": {},
  "standard": {
    "globals": [
      "atom",
      "jasmine",
      "describe",
      "xdescribe",
      "it",
      "waitsForPromise",
      "expect",
      "beforeEach",
      "runs"
    ]
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "release": {
    "verifyConditions": "condition-circle",
    "branch": "master"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "files": [
    "styles", "menus", "keymaps", "lib"
  ]
}
