{
  "name": "atom-jq",
  "main": "./lib/index.js",
  "description": "A playground for jq inside atom",
  "version": "0.6.1",
  "license": "MIT",
  "activationCommands": {},
  "repository": "https://github.com/sanack/atom-jq",
  "scripts": {
    "lint": "snazzy lib/ test/ spec/",
    "pretest": "npm run lint",
    "test": "babel-tape-runner ./test/*.test.js | tap-spec",
    "test:w": "nodemon --quiet --exec npm run test"
  },
  "engines": {
    "atom": "*"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "node-jq": "0.4.0",
    "path": "^0.12.7",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.5"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-tape-runner": "^2.0.1",
    "enzyme": "^2.3.0",
    "ghooks": "^1.3.2",
    "react-addons-test-utils": "^15.2.0",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "providedServices": {},
  "configSchema": {},
  "standard": {
    "globals": [
      "atom",
      "jasmine",
      "describe",
      "it",
      "waitsForPromise",
      "expect",
      "beforeEach",
      "runs"
    ]
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run test"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
